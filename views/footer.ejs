<div id="footer">
    <p>
    <%  //logged in
    if(typeof(user) == 'object' && user != null){  
        
        %>
    
        <a href="/logout?last=<%= name %>">Logout <%= user.displayName %></a> 
    <% } else { 
        //not logged in 
    %>
        <a href="/auth/github?last=<%= name %>">Login</a> with github...<br/>
    <% } %>
    </p>
</div>
