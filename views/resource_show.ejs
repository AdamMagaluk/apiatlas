<% include header %>
	<h1><%= r.title %></h1>
    <h2><%= r.description %></h2>
    <p><%= r.example %></p>

    <%  //logged in
    if(typeof(user) == 'number' && user != null){  %>
        <p>You voted: <%= r.your_vote %>. Vote: <a href="?vote=up">Up</a> or <a href="?vote=down">Down</a></p> 
    <% } else { /*not logged in*/  %>
        <a href="/auth/github?last=<%= name %>">Login</a> to vote.<br/>
    <% } %>
     
    <p><%= r.votes.up %>/<%= r.votes.total %></p>
    <p><a href=""></a></p>
<% include footer %>